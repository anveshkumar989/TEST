					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference",
						"parameters": {
							"Enter Database Name": {
								"value": "@pipeline().parameters.ENTER",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"ENTER": {
				"type": "string"
			}
		},
		"annotations": []
	}
