							},
							"sink": {
								"type": "AzureSqlSink",
								"disableMetricsCollection": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "empid",
											"type": "String"
										},
										"sink": {
											"name": "Empid",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "empname",
											"type": "String"
										},
										"sink": {
											"name": "empname",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "salary",
											"type": "String"
										},
										"sink": {
											"name": "salary",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "deptid",
											"type": "String"
										},
										"sink": {
											"name": "deptid",
											"type": "String"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "EMPBLOB",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": []
		}
	]
